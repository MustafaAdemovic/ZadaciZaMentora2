<script type="text/javascript"> 

	//append news Ids to rpc URLs
	if (typeof newsIds != "undefined" && newsIds.length > 0)
	{
		newsIdsSlug = '';
		for (i = 0; i < newsIds.length; i++)
			newsIdsSlug += '&newsIds[]=' + escape(parseInt(newsIds[i]));
			
		$('aside#tables_box.rounds_box_777_0 ul li a').each(
			function (i)
			{
				$(this).attr('href', $(this).attr('href') + newsIdsSlug);	
			}
			
		);
	}

   $.getScript("http://sportsport.ba/assets/js/rounds_box.js", function () 
	{
		roundsBox_init("777_0", roundBoxSelector_777_0, initialClicked_777_0, roundBox_previousRound_777_0, roundBox_nextRound_777_0);	
	}
	);

</script><aside id="tables_box" class="tables_box_1423335739">
